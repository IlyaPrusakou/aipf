managed implementation in class ZBP_R_PRU_MEM_MSG unique;
strict ( 2 );
extensible;
define behavior for ZR_PRU_MEM_MSG alias ZrPruMemMsg
persistent table zpru_mem_msg
extensible
lock master
authorization master ( global )
{
  field ( mandatory : create )
  AIPF7MessageUUID;

  field ( readonly : update )
  AIPF7MessageUUID;


  create;
  update;
  delete;

  mapping for zpru_mem_msg corresponding extensible
    {
      AIPF7MessageUUID      = messageuuid;
      AIPF7Content          = content;
      AIPF7MessageType      = messagetype;
      AIPF7MessageContentId = MessageContentId;
      AIPF7Stage            = stage;
      AIPF7SubStage         = substage;
      AIPF7Namespace        = namespace;
      AIPF7UserName         = username;
      AIPF7AgentUUID        = agentuuid;
      AIPF7RunUUID          = runuuid;
      AIPF7QueryUUID        = queryuuid;
      AIPF7StepUUID         = stepuuid;
      AIPF7MessageDateTime  = MessageDateTime;
      AIPF7CreatedBy        = createdby;
      AIPF7CreatedAt        = createdat;
      AIPF7ChangedBy        = changedby;
      AIPF7ChangedAt        = changedat;
    }

}